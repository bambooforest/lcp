Turn d
    FirstName = "Martin"
    LastName = "Schulz"
    OR
        test = test
        test != test

Segment@d s

sequence np
    Token@s t1
        upos = "DET"
    Token@s t2
        upos = "ADJ"
    Token@s t3
        lemma = /^[A-Z]/
        upos = "NOUN"

Token@s tv
    upos = "VERB"
    DepRel
        head = tv
        dep = t3
        label = /.*(OBJ|obj)/

myKWIC1 => plain
    context
        s
    entities
        tv
        np

stat1 => analysis
    attributes
        t1.lemma
        t2.lemma
        t3.lemma
    functions
        frequency
    filter
        frequency > 10


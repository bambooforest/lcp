Turn t
    Surname = "Schulz"

Segment@t s

sequence np
    Token@s t1
        upos = "VERB"
    Token@s t2
        upos = "DET"
    Token@s t3
        upos = "NOUN"

myKWIC1 => plain
    context
        s
    entities
        t1
        t2
        t3

Stat1 => analysis
    attributes
        t1.lemma
    functions
        frequency
    filter
        frequency > 1